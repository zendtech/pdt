--TEST--
Test formatting switch statements
--FILE--
<?php

switch($a)                  {
  case 1:
  break;
                          case 2:
                                                break;
default              :
                                                        break;
                                                        
                                                        }
                                                        
                                                        
                                              switch($b)                  {
                      case 1:
                      $i;
                      break;
                                              case 2:
                                              $j;
                                                                    break;
                    default              :
                                                                            break;
                                                        
                                                        }


switch ($a) {
case 1:
case 2:
case 3:
break;
}


?>
--EXPECT--
<?php
switch ($a) {
    case 1:
        break;
    case 2:
        break;
    default:
        break;
}
switch ($b) {
    case 1:
        $i;
        break;
    case 2:
        $j;
        break;
    default:
        break;
}
switch ($a) {
    case 1:
    case 2:
    case 3:
        break;
}
?>